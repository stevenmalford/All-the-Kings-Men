            <kind>1</kind>
            <string>switch(state)
{
    case eState.idle:
        sprite_index = sNecromancer_Idle;

        aggro = collision_circle(x, y, aggroRange, oPlayer, false, false);
        
        if(aggro)
        {
            state = eState.moving;
        }
        
        if(currenthp &lt;= 0)
        {
            state = eState.dead;
        }
        
        break;
        
    case eState.moving:
        sprite_index = sNecromancer;
        
        aggro = collision_circle(x, y, aggroRange, oPlayer, false, false);
        attack = collision_circle(x, y, attackRange, oPlayer, false, false);
        
        if(aggro || angry)
        {
            mp_potential_step(oPlayer.x, oPlayer.y, ms, false);
            angry = true;
        }
        
        if(currenthp &lt;= 0)
        {
            state = eState.dead;
        }
        
        if(attack)
        {
            state = eState.attacking;
        }
        
        break;
    
    case eState.attacking:
        sprite_index = sNecromancer_Idle;

        attack = collision_circle(x, y, attackRange, oPlayer, false, false);     
           
        if(!attack)
        {
            state = eState.moving;
        }
        
        if(resurrect = false)
        {
            resurrect = true;
            if(instance_exists(oParasite))
            {
                oParasite.resurrect = true;
            }
            if(instance_exists(oHeadhunter))
            {
                oHeadhunter.resurrect = true;
            }
            if(instance_exists(oShieldbreaker))
            {
                oShieldbreaker.resurrect = true;
            }
            if(instance_exists(oPlague_Puddle))
            {
                oPlague_Puddle.resurrect = true;
            }
            if(instance_exists(oRogue))
            {
                oRogue.resurrect = true;
            }
            alarm[0] = attackSpeed;
        }
        
        if(currenthp &lt;= 0)
        {
            state = eState.dead;
        }
        
        break;
    
    case eState.dead:
        sprite_index = sNecromancer;
        image_index = 0;
        
        dead_angle = 90;
        
        if(image_angle != dead_angle)
        {
            image_angle += 5;
        }
        
        if(dead = false)
        {
            alarm[1] = deathTimer;
            dead = true;
        }
        
        break;
}

if(direction &gt;= 90 &amp;&amp; direction &lt;= 270)
{
    image_xscale = 1;
}
else
{
    image_xscale = -1;
}
</string>
